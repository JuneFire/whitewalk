<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whitewalk.dao.IUserCollectionDao">

	<!-- 查询用户的标签数 -->
	<select id="findUserCollectionCountByUserId" resultType="int">
	
		select count(*) from userCollection where userId=#{userId}
	</select>

</mapper>